<div class="{__NODE_ID__}" instance="{__INSTANCE__}">

    <div class="inputs">
        <!-- input -->
        <div class="input" input_number="{NUMBER}" hover="hover">
            <div class="buttons">
                {DUPLICATE_BUTTON}
                {DELETE_BUTTON}
            </div>
            <div class="type">
                {TYPE_SELECT}
            </div>
            <div class="name">
                {NAME}
            </div>
            <div class="path">
                {PATH}
            </div>
            <div class="cb"></div>
            <div class="value {TYPE_CLASS}">
                {VALUE}
                <div class="cb"></div>
            </div>
            {NULL_BUTTON}
            <div class="cb"></div>
        </div>
        <!-- / -->
    </div>

    {CREATE_BUTTON}
    <div class="cb"></div>

</div>
